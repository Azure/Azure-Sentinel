    {
      "type": 3,
      "content": {
        "version": "KqlItem/1.0",
        "query": "AliCloud\n| where eventName in (\"ListAccessKeys\", \"GetAccountSummary\", \"GetPasswordPolicy\", \"GetAccountMFAInfo\")\n| summarize EventCount = count() by eventName\n| order by EventCount desc\n",
        "size": 4,
        "title": "High-Risk API Calls (Security Sensitive)",
        "timeContextFromParameter": "TimeGenrated",
        "showRefreshButton": true,
        "showExportToExcel": true,
        "queryType": 0,
        "resourceType": "microsoft.operationalinsights/workspaces",
        "visualization": "tiles",
        "tileSettings": {
          "titleContent": {
            "columnMatch": "eventName",
            "formatter": 1
          },
          "leftContent": {
            "columnMatch": "EventCount",
            "formatter": 12,
            "formatOptions": {
              "palette": "auto"
            },
            "numberFormat": {
              "unit": 17,
              "options": {
                "maximumSignificantDigits": 3,
                "maximumFractionDigits": 2
              }
            }
          },
          "showBorder": true
        }
      },
      "name": "High-Risk API Calls (Security Sensitive)"
    },
    {
      "type": 3,
      "content": {
        "version": "KqlItem/1.0",
        "query": "AliCloud\n| summarize EventCount = count() by serviceName\n| order by EventCount desc\n",
        "size": 4,
        "title": "Events by AliCloud Service Name",
        "timeContextFromParameter": "TimeGenrated",
        "showRefreshButton": true,
        "showExportToExcel": true,
        "queryType": 0,
        "resourceType": "microsoft.operationalinsights/workspaces",
        "visualization": "piechart",
        "chartSettings": {
          "showMetrics": false,
          "showLegend": true
        }
      },
      "customWidth": "33",
      "name": "Events by AliCloud Service Name"
    },
    {
      "type": 3,
      "content": {
        "version": "KqlItem/1.0",
        "query": "AliCloud\n| extend MFAStatus = tostring(parse_json(userIdentity.sessionContext.attributes).mfaAuthenticated)\n| summarize EventCount = count() by MFAStatus\n",
        "size": 4,
        "title": "Accounts Without MFA",
        "timeContextFromParameter": "TimeGenrated",
        "showRefreshButton": true,
        "showExportToExcel": true,
        "queryType": 0,
        "resourceType": "microsoft.operationalinsights/workspaces",
