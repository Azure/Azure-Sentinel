      "customWidth": "33",
      "name": "Accounts Without MFA"
    },
    {
      "type": 3,
      "content": {
        "version": "KqlItem/1.0",
        "query": "AliCloud\n| extend userType = tostring(userIdentity.type)\n| summarize EventCount = count() by userType\n",
        "size": 4,
