[
    {
        "name": "GCPCloudMonitoringPolling",
        "apiVersion": "2023-02-01-preview",
        "type": "Microsoft.SecurityInsights/dataConnectors",
        "location": "{{location}}",
        "kind": "RestApiPoller",
        "properties": {
            "connectorDefinitionName": "GCPMonitorCCPDefinition",
            "dcrConfig": {
                "dataCollectionEndpoint": "{{dataCollectionEndpoint}}",
                "dataCollectionRuleImmutableId": "{{dataCollectionRuleImmutableId}}",
                "streamName": "Custom-GCPMonitor"
            },
            "dataType": "GCP_MONITORINGV2_CL",
            "response": {
                "eventsJsonPaths": [
                    "$.timeSeries"
                ],
                "format": "json"
            },
            "paging": {
                "pagingType": "NextPageToken",
                "nextPageTokenJsonPath": "$.nextPageToken",
                "pageSize": 100,
                "PageSizeParaName": "pageSize",
                "PageTokenParaName": "pageToken"
            },
            "auth": {
                "type": "OAuth2",
                "IsClientSecretInHeader": "true",
                "IsCredentialsInHeaders": "true",
                "ClientSecret": "{{clientSecret}}",
                "ClientId": "{{clientId}}",
                "AuthorizationCode": "{{authorizationCode}}",
                "GrantType": "authorization_code",
                "TokenEndpoint": "https://oauth2.googleapis.com/token",
                "tokenEndpointHeaders": {
                    "Accept": "application/json",
                    "Content-Type": "application/x-www-form-urlencoded"
                },
                "authorizationEndpoint": "https://accounts.google.com/o/oauth2/auth",
                "authorizationEndpointQueryParameters": {
                    "prompt": "consent",
                    "access_type": "offline"
                },
                "scope": "https://www.googleapis.com/auth/monitoring.read",
                "redirectUri": "https://portal.azure.com/TokenAuthorize/ExtensionName/Microsoft_Azure_Security_Insights"
            },
            "request": {
                "apiEndpoint": "https://monitoring.googleapis.com/v3/projects/{{project_id}}/timeSeries",
                "httpMethod": "GET",
                "queryParameters": {
                    "filter": "metric.type=\"{{metric_type}}\"",
                "interval.startTime": "{_QueryWindowStartTime}",
                    "interval.endTime": "{_QueryWindowEndTime}",
                    "pageSize": "100",
                    "pageToken": "{_NextPageToken}"
                },
                "queryWindowInMin": 10,
                "queryTimeFormat": "yyyy-MM-ddTHH:mm:ss.Z",
                "StartTimeAttributeName": "interval.startTime",
                "EndTimeAttributeName": "interval.endTime",
                "rateLimitQPS": 10,
                "retryCount": 3,
                "timeoutInSeconds": 60,
                "headers": {
                    "Accept": "application/json"
                }
            },
            "isActive": true
        }
    }
]