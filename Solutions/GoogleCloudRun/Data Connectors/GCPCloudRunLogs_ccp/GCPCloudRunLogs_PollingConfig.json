[
    {
      "name": "GCPCloudRunLogs",
      "apiVersion": "2022-10-01-preview",
      "type": "Microsoft.SecurityInsights/dataConnectors",
      "kind": "GCP",
      "location": "{{location}}",
      "properties": {
        "connectorDefinitionName": "GCPCloudRunLogs_ConnectorDefinition",
        "dataType": "GCPCloudRunRequestLogs_CL",
        "dcrConfig": {
          "streamName": "Custom-GCPCloudRun",
          "dataCollectionEndpoint": "{{dataCollectionEndpoint}}",
          "dataCollectionRuleImmutableId": "{{dataCollectionRuleImmutableId}}"
        },
        "auth": {
          "serviceAccountEmail": "{{GCPServiceAccountEmail}}",
          "projectNumber": "{{GCPProjectNumber}}",
          "workloadIdentityProviderId": "{{GCPWorkloadIdentityProviderId}}"
        },
        "request": {
          "projectId": "{{GCPProjectId}}",
          "subscriptionNames": [
            "{{GCPSubscriptionName}}"
          ]
        }
      }
    }
  ]
  