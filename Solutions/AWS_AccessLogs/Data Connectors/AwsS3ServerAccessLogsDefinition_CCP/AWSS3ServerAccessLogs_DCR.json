    "location": "{{location}}",
    "properties": {
    "destinations": {
				"logAnalytics": [
					{
						"workspaceResourceId": "{{workspaceResourceId}}",
						"name": "clv2ws1"
					}
				]
			},
    "dataFlows": [
				{
					"streams": [
						"Microsoft-AWSS3ServerAccess"
					],
					"destinations": [
						"clv2ws1"
					]
                }
			],
            "dataCollectionEndpointId": "{{dataCollectionEndpointId}}"
    }
}