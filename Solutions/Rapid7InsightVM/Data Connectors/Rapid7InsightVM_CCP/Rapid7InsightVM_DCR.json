{
  "name": "Rapid7InsightVMDCR",
  "apiVersion": "2023-03-11",
  "type": "Microsoft.Insights/dataCollectionRules",
  "location": "{{location}}",
  "properties": {
    "dataCollectionEndpointId": "{{dataCollectionEndpointId}}",
    "destinations": {
      "logAnalytics": [
        {
          "workspaceResourceId": "{{workspaceResourceId}}",
          "name": "clv2ws1"
        }
      ]
    },
    "dataFlows": [
      {
        "streams": [
          "Microsoft-SentinelRapid7InsightVMCloudAssets"
        ],
        "destinations": [
          "clv2ws1"
        ]
      },
      {
        "streams": [
          "Microsoft-SentinelRapid7InsightVMCloudVulnerabilities"
        ],
        "destinations": [
          "clv2ws1"
        ]
      }
    ]
  }
}