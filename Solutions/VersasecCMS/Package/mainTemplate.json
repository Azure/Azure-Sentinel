                    },
                    {
                      "name": "SyslogID",
                      "type": "real"
                    },
                    {
                      "name": "SyslogIDCode",
                      "type": "string"
                    },
                    {
                      "name": "SyslogIDStrg",
                      "type": "string"
                    },
                    {
                      "name": "ID",
                      "type": "real"
                    },
                    {
                      "name": "ComputerName",
                      "type": "string"
                    },
                    {
                      "name": "CLID",
