                  "rateLimitQPS": 10,
                  "queryWindowInMin": "[parameters('pollingInterval')]",
                  "httpMethod": "POST",
                  "contentType": "application/json",
                  "queryTimeFormat": "yyyy-MM-ddTHH:mm:ss",
