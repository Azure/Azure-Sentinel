                                    {
                                        "name": "where[name]",
                                        "in": "query",
                                        "required": true,
                                        "type": "string",
                                        "x-ms-summary": "Domain name"
                                    },
                                    {
                                        "name": "Content-Type",
                                        "in": "header",
                                        "required": true,
                                        "type": "string",
                                        "default": "application/json",
                                        "x-ms-visibility": "internal"
                                    }
                                ]
                            }
                        },
                        "/1.0/domains/{DomainId}": {
                            "delete": {
                                "responses": {
                                    "default": {
                                        "description": "default",
                                        "schema": {}
                                    }
                                },
                                "summary": "Delete domain by id",
                                "operationId": "DeleteDomainById",
                                "parameters": [
                                    {
                                        "name": "DomainId",
                                        "in": "path",
                                        "required": true,
                                        "type": "string"
                                    },
                                    {
                                        "name": "Content-Type",
                                        "in": "header",
                                        "required": true,
                                        "type": "string",
                                        "default": "application/json",
                                        "x-ms-visibility": "internal"
                                    }
                                ],
                                "description": "Delete a domain from the shared customer’s domain list by Id."
