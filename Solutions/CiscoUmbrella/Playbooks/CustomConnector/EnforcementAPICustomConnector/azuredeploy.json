                                "required": [
                                    "alertTime",
                                    "deviceId",
                                    "deviceVersion",
                                    "dstDomain",
                                    "dstUrl",
                                    "eventTime",
                                    "protocolVersion",
                                    "providerName"
                                ],
                                "properties": {
                                    "alertTime": {
                                        "type": "string"
                                    },
                                    "deviceId": {
                                        "type": "string",
                                        "title": "deviceId",
                                        "x-ms-visibility": "internal",
                                        "default": "azuresentinel"
                                    },
                                    "deviceVersion": {
                                        "type": "string",
                                        "title": "deviceVersion",
