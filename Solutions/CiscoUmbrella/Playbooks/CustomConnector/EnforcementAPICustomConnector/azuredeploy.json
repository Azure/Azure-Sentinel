                                "parameters": [
                                    {
                                        "name": "page",
                                        "in": "query",
                                        "required": false,
                                        "type": "integer",
                                        "default": 1
                                    },
                                    {
                                        "name": "limit",
                                        "in": "query",
                                        "required": false,
                                        "type": "integer",
                                        "default": 200,
                                        "description": ""
                                    }
                                ]
                            },
                            "delete": {
                                "responses": {
