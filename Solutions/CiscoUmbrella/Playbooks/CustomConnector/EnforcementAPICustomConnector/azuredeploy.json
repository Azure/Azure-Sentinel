                                        "type": "string",
                                        "default": "1.0a",
                                        "title": "protocolVersion",
                                        "x-ms-visibility": "internal"
                                    },
                                    "providerName": {
                                        "type": "string",
                                        "default": "Security Platform",
                                        "title": "providerName",
                                        "x-ms-visibility": "internal"
                                    }
                                }
                            }
                        }
                    },
                    "securityDefinitions": {
                        "API Key": {
                            "type": "apiKey",
                            "in": "query",
                            "name": "customerKey"
                        }
                    },
                    "security": [
                        {
                            "API Key": []
                        }
                    ]
