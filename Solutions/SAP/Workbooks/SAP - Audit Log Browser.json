                    {
                        "type": 3,
                        "content": {
                            "version": "KqlItem/1.0",
                            "query": "SAPAuditLog \r\n| where TimeGenerated {TimeSelect}\r\n| where SystemID in ({Systems}) or  '*' in ({Systems}) \r\n| where AlertSeverityText <> ''\r\n| summarize Alerts = count() by bin(TimeGenerated, 1d)\r\n\r\n",
