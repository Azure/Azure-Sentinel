                            "query": "SAPAuditLog \r\n| where TimeGenerated {TimeSelect}\r\n| where SystemID in ({Systems}) or  '*' in ({Systems}) \r\n| where AlertSeverityText <> ''\r\n| where AlertSeverityText=='{status}'\r\n| where User=='{User}'\r\n| project TimeGenerated, MessageID=MessageID, Severity=AlertSeverityText, Message=MessageText\r\n| order by TimeGenerated desc\r\n| take 100",
                            "size": 1,
                            "title": "All events with {status} severity by user {User}",
