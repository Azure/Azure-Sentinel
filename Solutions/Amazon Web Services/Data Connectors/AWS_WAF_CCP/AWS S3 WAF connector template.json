                                                    }
                                                },
												{
                                                    "type": "CopyableLabel",
                                                    "parameters": {
                                                        "label": "Template 1: OpenID connect authentication deployment",
                                                        "isMultiLine": true,
														"fillWith": [
															"Oidc"
														]
                                                    }
                                                },
