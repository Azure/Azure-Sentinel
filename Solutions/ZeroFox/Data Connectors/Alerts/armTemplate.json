            {
              "metricName": "Total alerts received",
              "legend": "ZeroFox Alerts",
              "baseQuery": "ZeroFoxAlerts_CL"
            }
          ],
          "sampleQueries": [
            {
              "description": "List all ZeroFox alerts",
              "query": "ZeroFoxAlerts_CL\\n| sort by TimeGenerated asc"
            },
            {
              "description": "Count alerts by network type",
              "query": "ZeroFoxAlerts_CL\\n| summarize Count = count() by ThreatSource=network_s"
            },
            {
              "description": "Count alerts by entity",
              "query": "ZeroFoxAlerts_CL\\n| summarize Count = count() by Entity=entity_name_s"
            }
          ],
          "dataTypes": [
            {
              "name": "ZeroFoxAlerts_CL",
              "lastDataReceivedQuery": "ZeroFoxAlerts_CL\\n            | summarize Time = max(TimeGenerated)\\n            | where isnotempty(Time)"
            }
          ],
          "connectivityCriteria": [
