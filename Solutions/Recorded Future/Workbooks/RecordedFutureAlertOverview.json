        "version": "KqlItem/1.0",
        "query": "{log_table:value}\n| where RuleName_s in ({alert_rules:value})\n| distinct AlertID_s, RuleName_s, Triggered_t\n| summarize Alert=count() by bin(Triggered_t, 1h)\n",
        "size": 0,
        "title": "Alerts triggered over time",
