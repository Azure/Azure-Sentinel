      "_analyticRulecontentProductId1": "[concat(take(variables('_solutionId'),50),'-','ar','-', uniqueString(concat(variables('_solutionId'),'-','AnalyticsRule','-','4d173248-439b-4741-8b37-f63ad0c896ae','-', '1.0.4')))]"
