                                ]
                            },
                            "type": "InitializeVariable",
                            "inputs": {
                                "variables": [
                                    {
                                        "name": "co_located_ips_unique",
                                        "type": "array"
                                    }
                                ]
                            }
                        },
                        "rdata_hosts_output_": {
                            "runAfter": {
