                            "runAfter":  {
                                "Filter_array_for_Object_GUID_Entity_(InsightID)":  [
                                    "Succeeded"
                                ]
                            },
                            "type":  "Foreach"
                        },
                        "Parse_Entities_JSON":  {
                            "runAfter":  {
                            },
                            "type":  "ParseJson",
                            "inputs":  {
                                "content":  "@triggerBody()?['object']?['properties']?['relatedEntities']",
                                "schema":  {
                                    "items":  {
                                        "properties":  {
                                            "id":  {
