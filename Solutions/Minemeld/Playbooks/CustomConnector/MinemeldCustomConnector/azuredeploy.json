{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "CustomConnectorName": {
            "defaultValue": "MinemeldCustomConnector",
            "type": "String",
            "metadata": {
                "description": "Name of the custom connector, if you want to change the default name, make sure to use the same in all Minemeld automation playbooks as well"
            }
        },

        "MinemeldURL": {
            "defaultValue": "",
            "type": "String",
            "metadata": {
                "description": "Provide the minemeld URL, Do NOT prefix with https:// or http:// etc"
            }
        }
    },
    "variables": {
        "api_host": "[replace(replace(parameters('MinemeldURL'),'https://',''),'http://','')]",
        "ServiceName": "[concat('https://', variables('api_host'))]"
    },
    "resources": [
        {
            "type": "Microsoft.Web/customApis",
            "apiVersion": "2016-06-01",
            "name": "[parameters('CustomConnectorName')]",
            "location": "[resourceGroup().location]",
            "properties": {
                "connectionParameters": {
                    "username": {
                        "type": "securestring"
                    },
                    "password": {
                        "type": "securestring"
                    }
                },
                "description": "[concat('This connector helps to authenticate with ', parameters('CustomConnectorName'), ' and running Minemeld API queries')]",
                "displayName": "[parameters('CustomConnectorName')]",
                "iconUri": "data:image/png;base64,",
                "backendService": {
                    "serviceUrl": "[variables('ServiceName')]"
                },
                "apiType": "Rest",
                "swagger": {
                    "swagger": "2.0",
                    "info": {
                        "version": "1.0.0",
                        "title": "Minemeld",
                        "description": "Minemeld"
                    },
                    "host": "[replace(replace(parameters('MinemeldURL'),'https://',''),'http://','')]",
                    "basePath": "/",
                    "schemes": [ "https" ],
                    "consumes": [],
                    "produces": [ "application/json" ],
                    "paths": {
                        "/config/data/{indicator_name}/append": {
                            "post": {
                                "summary": "Add delete update indicator",
                                "description": "Add delete update indicator",
                                "operationId": "AddDeleteUpdateIndicator",
                                "parameters": [
                                    {
                                        "name": "h",
                                        "default": "local_Minerdb_name",
                                        "in": "query",
                                        "type": "string",
                                        "required": true
                                    },
                                    {
                                        "name": "t",
                                        "default": "localdb",
                                        "in": "query",
                                        "type": "string",
                                        "required": true
                                    },
                                    {
                                        "name": "indicator_name",
                                        "default": "your_miner_db_indicators",
                                        "in": "path",
                                        "type": "string",
                                        "required": true,
                                        "description": "For most cases it is minerdb name suffix with _indicators",
                                        "x-ms-summary": "Enter indicators name"
                                    },
                                    {
                                        "name": "Content-Type",
                                        "in": "header",
                                        "required": true,
                                        "type": "string",
                                        "default": "application/json",
                                        "description": "Content-Type"
                                    },
                                    {
                                        "name": "body",
                                        "in": "body",
                                        "schema": {
                                            "type": "object",
                                            "properties": {
                                                "indicator": {
                                                    "type": "string",
                                                    "description": "indicator",
                                                    "title": ""
                                                },
                                                "type": {
                                                    "type": "string",
                                                    "description": "type",
                                                    "title": "",
                                                    "enum": [ "IPv4", "IPv6", "URL", "domain", "user-id", "md5", "sha256", "sha1", "ssdeep", "email-addr" ]
                                                },
                                                "comment": {
                                                    "type": "string",
                                                    "description": "comment"
                                                },
                                                "share_level": {
                                                    "type": "string",
                                                    "description": "share_level"
                                                },
                                                "confidence": {
                                                    "type": "integer",
                                                    "format": "int32",
                                                    "description": "confidence"
                                                },
                                                "ttl": {
                                                    "type": "string",
                                                    "description": "\"Time to live\" : 0 for delete , else use \"disable\"",
                                                    "title": "ttl"
                                                }
                                            },
                                            "default": {
                                                "indicator": "8.8.8.1",
                                                "type": "IPv4",
                                                "comment": "usual Google DNS Public IP",
                                                "share_level": "green",
                                                "confidence": 100,
                                                "ttl": "disabled"
                                            },
                                            "required": [ "type", "indicator" ]
                                        },
                                        "required": true
                                    }
                                ],
                                "responses": {
                                    "default": {
                                        "description": "default",
                                        "schema": {}
                                    }
                                }
                            }
                        },
                        "/config/data/{indicator_name}": {
                            "get": {
                                "summary": "Get Indicators",
                                "description": "Get Indicators",
                                "operationId": "GetIndicators",
                                "parameters": [
                                    {
                                        "name": "t",
                                        "default": "localdb",
                                        "in": "query",
                                        "type": "string",
                                        "required": true
                                    },
                                    {
                                        "name": "indicator_name",
                                        "in": "path",
                                        "type": "string",
                                        "required": true,
                                        "description": "for most cases it is minerdb name suffix with _indicators",
                                        "x-ms-summary": "Enter indicators name",
                                        "default": "your_miner_db_indicator"
                                    }
                                ],
                                "responses": {
                                    "default": {
                                        "description": "default",
                                        "schema": {
                                            "type": "object",
                                            "properties": {
                                                "result": {
                                                    "type": "array",
                                                    "items": {
                                                        "type": "object",
                                                        "properties": {
                                                            "_expiration_ts": {
                                                                "type": "string",
                                                                "description": "_expiration_ts"
                                                            },
                                                            "_update_ts": {
                                                                "type": "integer",
                                                                "format": "int32",
                                                                "description": "_update_ts"
                                                            },
                                                            "comment": {
                                                                "type": "string",
                                                                "description": "comment"
                                                            },
                                                            "confidence": {
                                                                "type": "integer",
                                                                "format": "int32",
                                                                "description": "confidence"
                                                            },
                                                            "indicator": {
                                                                "type": "string",
                                                                "description": "indicator"
                                                            },
                                                            "share_level": {
                                                                "type": "string",
                                                                "description": "share_level"
                                                            },
                                                            "type": {
                                                                "type": "string",
                                                                "description": "type"
                                                            }
                                                        }
                                                    },
                                                    "description": "result"
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "definitions": {},
                    "parameters": {},
                    "responses": {},
                    "securityDefinitions": { "basic_auth": { "type": "basic" } },
                    "security": [ 
					{ 
					"basic_auth": [] 
					} 
					],
                    "tags": []
                }



            }
        }
    ]
}
