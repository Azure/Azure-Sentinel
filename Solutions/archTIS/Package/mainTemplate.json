{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "metadata": {
    "author": "archTIS",
    "comments": "Solution template for NC Protect Data Connector"
  },
  "parameters": {
    "location": {
      "type": "string",
      "minLength": 1,
      "defaultValue": "[resourceGroup().location]",
      "metadata": {
        "description": "Not used, but needed to pass arm-ttk test `Location-Should-Not-Be-Hardcoded`.  We instead use the `workspace-location` which is derived from the LA workspace"
      }
    },
    "workspace-location": {
      "type": "string",
      "defaultValue": "",
      "metadata": {
        "description": "[concat('Region to deploy solution resources -- separate from location selection',parameters('location'))]"
      }
    },
    "workspace": {
      "defaultValue": "",
      "type": "string",
      "metadata": {
        "description": "Workspace name for Log Analytics where Microsoft Sentinel is setup"
      }
    },
    "formattedTimeNow": {
      "type": "string",
      "defaultValue": "[utcNow('g')]",
      "metadata": {
        "description": "Appended to workbook displayNames to make them unique"
      }
    },
    "workbook1-id": {
      "type": "string",
      "defaultValue": "[newGuid()]",
      "minLength": 1,
      "metadata": {
        "description": "Unique id for the workbook"
      }
    },
    "workbook1-name": {
      "type": "string",
      "defaultValue": "NCProtect",
      "minLength": 1,
      "metadata": {
        "description": "Name for the workbook"
      }
    },
    "analyticsRule-id": {
      "type": "string",
      "defaultValue": "[newGuid()]",
      "metadata": {
        "description": "Unique id for the analytics rule"
      }
    },
    "automationRuleHigh-id": {
      "type": "string",
      "defaultValue": "[newGuid()]",
      "metadata": {
        "description": "Unique id for HIGH severity automation rule"
      }
    },
    "automationRuleMedium-id": {
      "type": "string",
      "defaultValue": "[newGuid()]",
      "metadata": {
        "description": "Unique id for MEDIUM severity automation rule"
      }
    },
    "automationRuleLow-id": {
      "type": "string",
      "defaultValue": "[newGuid()]",
      "metadata": {
        "description": "Unique id for LOW severity automation rule"
      }
    },
    "automationRuleInfo-id": {
      "type": "string",
      "defaultValue": "[newGuid()]",
      "metadata": {
        "description": "Unique id for INFORMATIONAL severity automation rule"
      }
    },
    "watchlist-displayName": {
      "type": "string",
      "defaultValue": "wNCPFileDownloadMonitoring",
      "metadata": {
        "description": "Display name for the watchlist"
      }
    }
  },
  "variables": {
    "workbook-source": "[concat(resourceGroup().id, '/providers/Microsoft.OperationalInsights/workspaces/',parameters('workspace'))]",
    "_workbook-source": "[variables('workbook-source')]",
    "solutionId": "nucleuscyber.nc-protect-azure-sentinel-data-connector",
    "_solutionId": "[variables('solutionId')]",
    "workspaceResourceId": "[resourceId('microsoft.OperationalInsights/Workspaces', parameters('workspace'))]",
    "uiConfigId1": "NCProtect",
    "_uiConfigId1": "[variables('uiConfigId1')]",
    "dataConnectorContentId1": "NCProtect",
    "_dataConnectorContentId1": "[variables('dataConnectorContentId1')]",
    "dataConnectorId1": "[extensionResourceId(resourceId('Microsoft.OperationalInsights/workspaces', parameters('workspace')), 'Microsoft.SecurityInsights/dataConnectors', variables('_dataConnectorContentId1'))]",
    "_dataConnectorId1": "[variables('dataConnectorId1')]",
    "dataConnectorTemplateSpecName1": "[concat(parameters('workspace'),'-dc-',uniquestring(variables('_dataConnectorContentId1')))]",
    "dataConnectorVersion1": "8.0.0",
    "workbookVersion1": "1.0.0",
    "workbookContentId1": "NCProtectWorkbook",
    "workbookId1": "[resourceId('Microsoft.Insights/workbooks', variables('workbookContentId1'))]",
    "workbookTemplateSpecName1": "[concat(parameters('workspace'),'-wb-',uniquestring(variables('_workbookContentId1')))]",
    "_workbookContentId1": "[variables('workbookContentId1')]"
  },
  "resources": [
    {
      "metadata": {
        "workspace": "[parameters('workspace')]"
      },
      "type": "Microsoft.Insights/workbooks",
      "name": "[parameters('workbook1-id')]",
      "location": "[parameters('workspace-location')]",
      "kind": "shared",
      "apiVersion": "2023-06-01",
      "properties": {
        "displayName": "[concat(parameters('workbook1-name'), ' - ', parameters('formattedTimeNow'))]",
        "serializedData": "{\"version\":\"Notebook/1.0\",\"items\":[{\"type\":1,\"content\":{\"json\":\"NC Protect\"},\"name\":\"text - 2\"},{\"type\":12,\"content\":{\"version\":\"NotebookGroup/1.0\",\"groupType\":\"editable\",\"items\":[{\"type\":3,\"content\":{\"version\":\"KqlItem/1.0\",\"query\":\"NCProtectUAL_CL | where TimeGenerated > ago(7d) | order by TimeGenerated desc \",\"size\":0,\"title\":\"Get last 7 days records\",\"timeContext\":{\"durationMs\":259200000},\"queryType\":0,\"resourceType\":\"microsoft.operationalinsights/workspaces\"},\"customWidth\":\"33\",\"name\":\"query - 1\",\"styleSettings\":{\"showBorder\":true}},{\"type\":3,\"content\":{\"version\":\"KqlItem/1.0\",\"query\":\"NCProtectUAL_CL | where TimeGenerated > ago(1h) and Type_s == 'LoginFailure' | summarize FailedRequestCount = count() by bin(TimeGenerated, 1h), UserDisplayName_s, UserEmail_s, UserLoginName_s, Type_s, JSONExtra_s | where  FailedRequestCount > 3 \",\"size\":0,\"title\":\"Login failed consecutively for more than 3 times in an hour by user\",\"timeContext\":{\"durationMs\":2592000000},\"queryType\":0,\"resourceType\":\"microsoft.operationalinsights/workspaces\"},\"customWidth\":\"33\",\"name\":\"query - 2\",\"styleSettings\":{\"showBorder\":true}},{\"type\":3,\"content\":{\"version\":\"KqlItem/1.0\",\"query\":\"NCProtectUAL_CL | where TimeGenerated > ago(1h) and Type_s == 'Open' and Status_s == 'Fail' | summarize FailedRequestCount = count() by bin(TimeGenerated, 1h), UserDisplayName_s, UserEmail_s, UserLoginName_s, Type_s, JSONExtra_s, DocumentUrl_s | where  FailedRequestCount > 3 \",\"size\":0,\"title\":\"Download failed consecutively for more than 3 times in an hour by user\",\"timeContext\":{\"durationMs\":2592000000},\"queryType\":0,\"resourceType\":\"microsoft.operationalinsights/workspaces\"},\"customWidth\":\"33\",\"name\":\"query - 3\",\"styleSettings\":{\"showBorder\":true}},{\"type\":3,\"content\":{\"version\":\"KqlItem/1.0\",\"query\":\"NCProtectUAL_CL | where TimeGenerated > ago(7d) and (Type_s == 'Create' or Type_s == 'Modify' or Type_s == 'Delete') and isnotempty(RuleName_s) | order by TimeGenerated desc \",\"size\":0,\"title\":\"Get logs for rule created or modified or deleted records in last 7 days\",\"timeContext\":{\"durationMs\":2592000000},\"queryType\":0,\"resourceType\":\"microsoft.operationalinsights/workspaces\"},\"customWidth\":\"33\",\"name\":\"query - 4\",\"styleSettings\":{\"showBorder\":true}}]},\"name\":\"group - 0\"}]}\r\n",
        "version": "1.0",
        "sourceId": "[variables('_workbook-source')]",
        "category": "sentinel"
      }
    },
    {
      "type": "Microsoft.Resources/templateSpecs",
      "apiVersion": "2022-02-01",
      "name": "[variables('dataConnectorTemplateSpecName1')]",
      "location": "[parameters('workspace-location')]",
      "tags": {
        "hidden-sentinelWorkspaceId": "[variables('workspaceResourceId')]",
        "hidden-sentinelContentType": "DataConnector"
      },
      "properties": {
        "description": "NC Protect Data Connector data connector with template",
        "displayName": "NC Protect Data Connector template"
      }
    },
    {
      "type": "Microsoft.OperationalInsights/workspaces/providers/watchlists",
      "apiVersion": "2023-11-01",
      "name": "[concat(parameters('workspace'),'/Microsoft.SecurityInsights/', 'NCPFDM')]",
      "properties": {
        "displayName": "[parameters('watchlist-displayName')]",
        "rawContent": "RuleName,DocumentDownloadThreshold,TimeWindowMinutes,IncidentSeverity\r\nrNCPFileDownloadMonitoring,5,60,Medium\r\n",
        "provider": "Microsoft",
        "source": "Local file",
        "sourceType": "Local",
        "numberOfLinesToSkip": 0,
        "itemsSearchKey": "RuleName",
        "contentType": "text/csv",
        "description": "Configuration watchlist for NC Protect file download monitoring thresholds"
      }
    },
    {
      "type": "Microsoft.OperationalInsights/workspaces/providers/alertRules",
      "apiVersion": "2023-12-01-preview",
      "name": "[concat(parameters('workspace'),'/Microsoft.SecurityInsights/', parameters('analyticsRule-id'))]",
      "dependsOn": [
        "[extensionResourceId(resourceId('Microsoft.OperationalInsights/workspaces', parameters('workspace')), 'Microsoft.SecurityInsights/watchlists', 'NCPFDM')]"
      ],
      "kind": "NRT",
      "location": "[parameters('workspace-location')]",
      "properties": {
        "displayName": "DownloadRateHighRule",
        "description": "Monitors high download rates based on configurable thresholds from watchlist",
        "severity": "Medium",
        "enabled": false,
        "query": "let NCPRuleConfig = _GetWatchlist('NCPFDM')\r\n| where RuleName == \"rNCPFileDownloadMonitoring\"\r\n| top 1 by RuleName\r\n| project \r\n\tDocumentDownloadThreshold = toint(DocumentDownloadThreshold),\r\n\tTimeWindowMinutes = toint(TimeWindowMinutes),\r\n\tIncidentSeverity = tostring(IncidentSeverity);\r\n\r\nlet downloadThreshold = toscalar(NCPRuleConfig | project DocumentDownloadThreshold);\r\nlet timeWindowMinutes = toscalar(NCPRuleConfig | project TimeWindowMinutes);\r\nlet incidentSeverity = toscalar(NCPRuleConfig | project IncidentSeverity);\r\n\r\nNCProtectUAL_CL\r\n| where TimeGenerated > ago(timeWindowMinutes * 1m)\r\n| where Type_s == \"Download\"\r\n| summarize\r\n    DownloadedDocuments = make_list(DocumentUrl_s, 50),\r\n    DownloadCount = count(),\r\n    FirstEvent = format_datetime(min(TimeGenerated), 'yyyy-MM-dd HH:mm:ss') ,\r\n    LastEvent = format_datetime(max(TimeGenerated), 'yyyy-MM-dd HH:mm:ss'),\r\n\tActiveUsers = make_set(UserEmail_s)\r\n    by UserLoginName_s, UserEmail_s\r\n| where DownloadCount > downloadThreshold\r\n| extend\r\n\tConfiguredDownloadThreshold = downloadThreshold,\r\n\tConfiguredTimeWindow = strcat(timeWindowMinutes, \"m\"),\r\n\tConfiguredIncidentSeverity = incidentSeverity\r\n| project \r\n\tUserLoginName_s,\r\n    UserEmail_s,\r\n    DownloadCount,\r\n    DownloadedDocuments,\r\n    FirstEvent,\r\n    LastEvent,\r\n    ActiveUsers,\r\n \tConfiguredDownloadThreshold,\r\n\tConfiguredTimeWindow,\r\n\tConfiguredIncidentSeverity",
        "suppressionDuration": "PT5H",
        "suppressionEnabled": false,
        "alertRuleTemplateName": null,
        "incidentConfiguration": {
          "createIncident": true,
          "groupingConfiguration": {
            "enabled": false,
            "reopenClosedIncident": false,
            "lookbackDuration": "PT1H",
            "matchingMethod": "AllEntities"
          }
        },
        "eventGroupingSettings": {
          "aggregationKind": "AlertPerResult"
        },
        "alertDetailsOverride": {
          "alertDisplayNameFormat": "NCP-{{UserEmail_s}}-{{DownloadCount}}-downloaded-documents",
          "alertDescriptionFormat": "Document Download High rate ( {{DownloadCount}} ) detected within {{FirstEvent}} : {{LastEvent}} "
        },
        "customDetails": {
          "DynSeverity": "ConfiguredIncidentSeverity",
          "DynThreshold": "ConfiguredDownloadThreshold",
          "DynDownloadWindow": "ConfiguredTimeWindow"
        },
        "entityMappings": null,
        "sentinelEntitiesMappings": null,
        "templateVersion": null
      }
    },
    {
      "type": "Microsoft.OperationalInsights/workspaces/providers/automationRules",
      "apiVersion": "2023-12-01-preview",
      "name": "[concat(parameters('workspace'),'/Microsoft.SecurityInsights/', parameters('automationRuleHigh-id'))]",
      "dependsOn": [
        "[resourceId('Microsoft.OperationalInsights/workspaces/providers/alertRules', parameters('workspace'), 'Microsoft.SecurityInsights', parameters('analyticsRule-id'))]"
      ],
      "properties": {
        "displayName": "FileDownloadedHighRateAR_Severity_HIGH",
        "order": 1,
        "triggeringLogic": {
          "isEnabled": true,
          "triggersOn": "Incidents",
          "triggersWhen": "Created",
          "conditions": [
            {
              "conditionType": "PropertyArray",
              "conditionProperties": {
                "arrayType": "CustomDetails",
                "arrayConditionType": "AnyItem",
                "itemConditions": [
                  {
                    "conditionType": "PropertyArray",
                    "conditionProperties": {
                      "arrayType": "CustomDetailValues",
                      "arrayConditionType": "AnyItem",
                      "itemConditions": [
                        {
                          "conditionType": "Property",
                          "conditionProperties": {
                            "propertyName": "IncidentCustomDetailsValue",
                            "operator": "Contains",
                            "propertyValues": [
                              "High",
                              "high",
                              "HIGH"
                            ]
                          }
                        }
                      ]
                    }
                  },
                  {
                    "conditionType": "Property",
                    "conditionProperties": {
                      "propertyName": "IncidentCustomDetailsKey",
                      "operator": "Equals",
                      "propertyValues": [
                        "DynSeverity"
                      ]
                    }
                  }
                ]
              }
            }
          ]
        },
        "actions": [
          {
            "order": 1,
            "actionType": "ModifyProperties",
            "actionConfiguration": {
              "severity": "High"
            }
          }
        ]
      }
    },
    {
      "type": "Microsoft.OperationalInsights/workspaces/providers/automationRules",
      "apiVersion": "2023-12-01-preview",
      "name": "[concat(parameters('workspace'),'/Microsoft.SecurityInsights/', parameters('automationRuleMedium-id'))]",
      "dependsOn": [
        "[resourceId('Microsoft.OperationalInsights/workspaces/providers/alertRules', parameters('workspace'), 'Microsoft.SecurityInsights', parameters('analyticsRule-id'))]"
      ],
      "properties": {
        "displayName": "FileDownloadedHighRateAR_Severity_MEDIUM",
        "order": 2,
        "triggeringLogic": {
          "isEnabled": true,
          "triggersOn": "Incidents",
          "triggersWhen": "Created",
          "conditions": [
            {
              "conditionType": "PropertyArray",
              "conditionProperties": {
                "arrayType": "CustomDetails",
                "arrayConditionType": "AnyItem",
                "itemConditions": [
                  {
                    "conditionType": "PropertyArray",
                    "conditionProperties": {
                      "arrayType": "CustomDetailValues",
                      "arrayConditionType": "AnyItem",
                      "itemConditions": [
                        {
                          "conditionType": "Property",
                          "conditionProperties": {
                            "propertyName": "IncidentCustomDetailsValue",
                            "operator": "Contains",
                            "propertyValues": [
                              "Medium",
                              "medium",
                              "MEDIUM"
                            ]
                          }
                        }
                      ]
                    }
                  },
                  {
                    "conditionType": "Property",
                    "conditionProperties": {
                      "propertyName": "IncidentCustomDetailsKey",
                      "operator": "Equals",
                      "propertyValues": [
                        "DynSeverity"
                      ]
                    }
                  }
                ]
              }
            }
          ]
        },
        "actions": [
          {
            "order": 1,
            "actionType": "ModifyProperties",
            "actionConfiguration": {
              "severity": "Medium"
            }
          }
        ]
      }
    },
    {
      "type": "Microsoft.OperationalInsights/workspaces/providers/automationRules",
      "apiVersion": "2023-12-01-preview",
      "name": "[concat(parameters('workspace'),'/Microsoft.SecurityInsights/', parameters('automationRuleLow-id'))]",
      "dependsOn": [
        "[resourceId('Microsoft.OperationalInsights/workspaces/providers/alertRules', parameters('workspace'), 'Microsoft.SecurityInsights', parameters('analyticsRule-id'))]"
      ],
      "properties": {
        "displayName": "FileDownloadedHighRateAR_Severity_LOW",
        "order": 3,
        "triggeringLogic": {
          "isEnabled": true,
          "triggersOn": "Incidents",
          "triggersWhen": "Created",
          "conditions": [
            {
              "conditionType": "PropertyArray",
              "conditionProperties": {
                "arrayType": "CustomDetails",
                "arrayConditionType": "AnyItem",
                "itemConditions": [
                  {
                    "conditionType": "PropertyArray",
                    "conditionProperties": {
                      "arrayType": "CustomDetailValues",
                      "arrayConditionType": "AnyItem",
                      "itemConditions": [
                        {
                          "conditionType": "Property",
                          "conditionProperties": {
                            "propertyName": "IncidentCustomDetailsValue",
                            "operator": "Contains",
                            "propertyValues": [
                              "Low",
                              "low",
                              "LOW"
                            ]
                          }
                        }
                      ]
                    }
                  },
                  {
                    "conditionType": "Property",
