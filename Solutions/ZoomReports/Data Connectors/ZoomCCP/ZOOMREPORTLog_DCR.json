[
    {
        "name": "ZoomReportDCR",
        "apiVersion": "2021-09-01-preview",
        "type": "Microsoft.Insights/dataCollectionRules",
        "location": "{{location}}",
        "properties": {
            "dataCollectionEndpointId": "{{dataCollectionEndpoint}}",
            "streamDeclarations": {
                "Custom-Zoom_Operations": {
                    "columns": [
                        {
                            "name": "time",
                            "type": "datetime"
                        },
                        {
                            "name": "operator",
                            "type": "string"
                        },
                        {
                            "name": "category_type",
                            "type": "string"
                        },
                        {
                            "name": "action",
                            "type": "string"
                        },
                        {
                            "name": "operation_detail",
                            "type": "string"
                        }
                    ]
                },
                "Custom-Zoom_Activities": {
                    "columns": [
                        {
                            "name": "email",
                            "type": "string"
                        },
                        {
                            "name": "time",
                            "type": "datetime"
                        },
                        {
                            "name": "type",
                            "type": "string"
                        },
                        {
                            "name": "ip_address",
                            "type": "string"
                        },
                        {
                            "name": "client_type",
                            "type": "string"
                        },
                        {
                            "name": "version",
                            "type": "string"
                        }
                    ]
                },
                "Custom-Zoom_Users": {
                    "columns": [
                        {
                            "name": "custom_attributes",
                            "type": "dynamic"
                        },
                        {
                            "name": "dept",
                            "type": "string"
                        },
                        {
                            "name": "email",
                            "type": "string"
                        },
                        {
                            "name": "id",
                            "type": "string"
                        },
                        {
                            "name": "meeting_minutes",
                            "type": "int"
                        },
                        {
                            "name": "meetings",
                            "type": "int"
                        },
                        {
                            "name": "participants",
                            "type": "int"
                        },
                        {
                            "name": "last_client_version",
                            "type": "string"
                        },
                        {
                            "name": "last_login_time",
                            "type": "datetime"
                        },
                        {
                            "name": "create_time",
                            "type": "datetime"
                        },
                        {
                            "name": "type",
                            "type": "int"
                        },
                        {
                            "name": "user_name",
                            "type": "string"
                        }
                    ]
                },
                "Custom-Zoom_TelephoneReports": {
                    "columns": [
                        {
                            "name": "call_in_number",
                            "type": "string"
                        },
                        {
                            "name": "country_name",
                            "type": "string"
                        },
                        {
                            "name": "dept",
                            "type": "string"
                        },
                        {
                            "name": "duration",
                            "type": "int"
                        },
                        {
                            "name": "end_time",
                            "type": "datetime"
                        },
                        {
                            "name": "host_email",
                            "type": "string"
                        },
                        {
                            "name": "host_id",
                            "type": "string"
                        },
                        {
                            "name": "host_name",
                            "type": "string"
                        },
                        {
                            "name": "meeting_id",
                            "type": "int"
                        },
                        {
                            "name": "meeting_type",
                            "type": "string"
                        },
                        {
                            "name": "phone_number",
                            "type": "string"
                        },
                        {
                            "name": "rate",
                            "type": "int"
                        },
                        {
                            "name": "signaled_number",
                            "type": "string"
                        },
                        {
                            "name": "start_time",
                            "type": "datetime"
                        },
                        {
                            "name": "total",
                            "type": "int"
                        },
                        {
                            "name": "type",
                            "type": "string"
                        },
                        {
                            "name": "uuid",
                            "type": "string"
                        }
                    ]
                },
                "Custom-Zoom_CloudRecordings": {
                    "columns": [
                        {
                            "name": "date",
                            "type": "datetime"
                        },
                        {
                            "name": "free_usage",
                            "type": "string"
                        },
                        {
                            "name": "plan_usage",
                            "type": "string"
                        },
                        {
                            "name": "usage",
                            "type": "string"
                        }
                    ]
                },
                "Custom-Zoom_DailyReports": {
                    "columns": [
                        {
                            "name": "date",
                            "type": "datetime"
                        },
                        {
                            "name": "meeting_minutes",
                            "type": "int"
                        },
                        {
                            "name": "meetings",
                            "type": "int"
                        },
                        {
                            "name": "new_users",
                            "type": "int"
                        },
                        {
                            "name": "participants",
                            "type": "int"
                        }
                    ]
                }
            },
            "destinations": {
                "logAnalytics": [
                    {
                        "workspaceResourceId": "{{workspaceResourceId}}",
                        "name": "clv2ws1"
                    }
                ]
            },
            "dataFlows": [
                {
                    "streams": [
                        "Custom-Zoom_Operations"
                    ],
                    "destinations": [
                        "clv2ws1"
                    ],
                    "transformKql": "source\n| project-rename TimeGenerated = ['time']\n",
                    "outputStream": "Custom-Zoom_Operations_CL"
                },
                {
                    "streams": [
                        "Custom-Zoom_Activities"
                    ],
                    "destinations": [
                        "clv2ws1"
                    ],
                    "transformKql": "source\n| project-rename TimeGenerated= ['time'], EventType=type\n\n",
                    "outputStream": "Custom-Zoom_Activities_CL"
                },
                {
                    "streams": [
                        "Custom-Zoom_Users"
                    ],
                    "destinations": [
                        "clv2ws1"
                    ],
                    "transformKql": "source\n| extend TimeGenerated= now() | project-rename userType=type, userId=id\n\n",
                    "outputStream": "Custom-Zoom_Users_CL"
                },
                {
                    "streams": [
                        "Custom-Zoom_TelephoneReports"
                    ],
                    "destinations": [
                        "clv2ws1"
                    ],
                    "transformKql": "source\n| extend TimeGenerated= now() | project-rename callType=type, meetingUuid=uuid \n\n",
                    "outputStream": "Custom-Zoom_TelephoneReports_CL"
                },
                {
                    "streams": [
                        "Custom-Zoom_CloudRecordings"
                    ],
                    "destinations": [
                        "clv2ws1"
                    ],
                    "transformKql": "source\n| extend TimeGenerated= now() \n\n",
                    "outputStream": "Custom-Zoom_CloudRecordings_CL"
                },
                {
                    "streams": [
                        "Custom-Zoom_DailyReports"
                    ],
                    "destinations": [
                        "clv2ws1"
                    ],
                    "transformKql": "source\n| extend TimeGenerated= now() | project-rename reportDate=['date'] \n\n",
                    "outputStream": "Custom-Zoom_DailyReports_CL"
                }
            ]
        }
    }
]