        "query": "ZNSegmentAudit\n| where isnotempty(PerformedByName) and PerformedByName != \"Zero Networks\"\n| summarize count() by PerformedByName\n| order by count_\n| project PerformedByName, EventCount=count_",
        "size": 0,
