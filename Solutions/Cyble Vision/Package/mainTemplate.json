{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workspace": {
      "type": "string",
      "metadata": {
        "description": "Log Analytics workspace name"
      }
    },
    "location": {
      "type": "string",
      "defaultValue": "[resourceGroup().location]",
      "metadata": {
        "description": "Location for all resources."
      }
    },
    "cyble_api_key": {
      "type": "string",
      "metadata": {
        "description": "Cyble API Key for authentication"
      }
    },
    "iocEnrichmentDefinition": {
      "type": "object",
      "defaultValue": {},
      "metadata": {
        "description": "Definition for the Cyble IOC Enrichment playbook"
      }
    },
    "threatIntelDefinition": {
      "type": "object",
      "defaultValue": {},
      "metadata": {
        "description": "Definition for the Cyble Threat Intelligence Ingestion playbook"
      }
    }
  },
  "resources": [
    {
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2023-07-01",
      "name": "CybleIOCEnrichmentDeployment",
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "[uri(deployment().properties.templateLink.uri, 'Playbooks/IoC-Enrichment/Cyble_IOC_Enrichment.json')]",
          "contentVersion": "1.0.0.0"
        },
        "parameters": {
          "cyble_api_key": {
            "value": "[parameters('cyble_api_key')]"
          },
          "location": {
            "value": "[parameters('location')]"
          },
          "workspace": {
            "value": "[parameters('workspace')]"
          }
        }
      }
    },
    {
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2023-07-01",
      "name": "CybleTIIngestDeployment",
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "[uri(deployment().properties.templateLink.uri, 'Playbooks/TI-Ingest/Cyble_TI_Ingest.json')]",
          "contentVersion": "1.0.0.0"
        },
        "parameters": {
          "cyble_api_key": {
            "value": "[parameters('cyble_api_key')]"
          },
          "location": {
            "value": "[parameters('location')]"
          },
          "workspace": {
            "value": "[parameters('workspace')]"
          }
        }
      }
    }
  ],
  "outputs": {}
}
