[
    {
        "name": "CybleVisionPoller",
        "apiVersion": "2023-02-01-preview",
        "type": "Microsoft.SecurityInsights/dataConnectors",
        "location": "{{location}}",
        "kind": "RestApiPoller",
        "properties": {
            "connectorDefinitionName": "CybleVisionAlerts",
            "dataType": "CybleVisionAlerts_CL",
            "dcrConfig": {
                "streamName": "Custom-CybleVisionAlerts_CL",
                "dataCollectionEndpoint": "{{dataCollectionEndpoint}}",
                "dataCollectionRuleImmutableId": "{{dataCollectionRuleImmutableId}}"
            },
            "auth": {
                "type": "APIKey",
                "ApiKey": "{{ApiToken}}",
                "ApiKeyName": "Authorization",
                "ApiKeyIdentifier": "Bearer"
            },
            "request": {
                "apiEndpoint": "https://bifrost.cyble.ai/engine/api/v1/y/tpi/cortex/alerts",
                "httpMethod": "Post",
                "queryWindowInMin": 5,
                "queryTimeFormat": "yyyy-MM-ddTHH:mm:ssZ",
                "rateLimitQps": 1,
                "retryCount": 3,
                "timeoutInSeconds": 60,
                "headers": {
                    "Content-Type": "application/json"
                },
                    "queryParametersTemplate": "{\"limit\": 1000, \"start_time\": \"{last_run_time}\", \"end_time\": \"{current_time}\" }",
                "isPostPayloadJson": true
            },
            "response": {
                "format": "json",
                "eventsJsonPaths": [
                    "$.items"
                ]
            },
            "paging": {
                "pagingType": "NextPageToken",
                "nextPageParaName": "cursor",
                "nextPageTokenJsonPath": "$.cursor",
                "hasNextFlagJsonPath": "$.has_more"
            }
        }
    }
]