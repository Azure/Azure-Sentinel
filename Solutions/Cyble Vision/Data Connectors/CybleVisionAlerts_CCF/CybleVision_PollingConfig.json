[
    {
        "name": "CybleVisionPoller",
        "apiVersion": "2023-02-01-preview",
        "type": "Microsoft.SecurityInsights/dataConnectors",
        "location": "{{location}}",
        "kind": "RestApiPoller",
        "properties": {
            "connectorDefinitionName": "CybleVisionAlerts",
            "dataType": "CybleVisionAlerts_CL",
            "dcrConfig": {
                "streamName": "Custom-CybleVisionAlerts_CL",
                "dataCollectionEndpoint": "{{dataCollectionEndpoint}}",
                "dataCollectionRuleImmutableId": "{{dataCollectionRuleImmutableId}}"
            },
            "auth": {
                "type": "APIKey",
                "ApiKey": "[[parameters('ApiToken')]",
                "ApiKeyName": "Authorization",
                "ApiKeyIdentifier": "Bearer"
            },
            "request": {
                "apiEndpoint": "https://bifrost.cyble.ai/ar-apollo-v2/api/v2/y/alerts",
                "httpMethod": "POST",
                "queryWindowInMin": 10,
                "queryTimeFormat": "yyyy-MM-ddTHH:mm:ssZ",
                "rateLimitQps": 1,
                "retryCount": 3,
                "timeoutInSeconds": 60,
                "headers": {
                    "Content-Type": "application/json",
                    "Accept": "application/json"
                },
                "isPostPayloadJson": true,
                "queryParametersTemplate": "{ \"orderBy\": [ { \"created_at\": \"desc\" }, { \"updated_at\": \"desc\" } ], \"filters\": { \"service\": [ \"darkweb_data_breaches\", \"github\" ], \"created_at\": { \"gte\": \"{_QueryWindowStartTime}\", \"lte\": \"{_QueryWindowEndTime}\" }, \"status\": [ \"UNREVIEWED\" ], \"user_severity\": [ \"LOW\", \"MEDIUM\", \"HIGH\" ] }, \"excludes\": { \"status\": [ \"FALSE_POSITIVE\" ] }, \"skip\": 0, \"countOnly\": false, \"taggedAlert\": false, \"withDataMessage\": false }"
            },
            "response": {
                "format": "json",
                "eventsJsonPaths": [
                    "$.data.darkweb_data_breaches",
                    "$.data.github"
                ]
            }
        }
    }
]
