{
  "SolutionName": "Pathlock Threat Detection and Response for Microsoft Sentinel",
  "SolutionAuthor": "Pathlock Inc.",
  "SolutionVersion": "3.2.1",
  "PackageId": "azuresentinel.azure-sentinel-solution-Pathlock_TDnR_PushV1",
  "TemplateName": "Pathlock_TDnR_PushV1",
  "ConnectorDefinitionTemplateVersion": "1.0.0",
  "DataConnectorsTemplateVersion": "1.0.0",
  "PackageIcon": "Pathlock",
  "SolutionTier": "Partner",

    "parameters": {
    "workspaceName": { "type": "string" }
  },
  "variables": {
    "contentProductName": "[concat(take(variables('_solutionId'), 50), '-content-', uniqueString(variables('_solutionId')))]",
    "_solutioncontentProductId": "[[extensionResourceId(resourceId('Microsoft.OperationalInsights/workspaces', parameters('workspaceName')), 'Microsoft.SecurityInsights/contentProductPackages', variables('contentProductName'))]]",
    "_dataConnectorContentProductId_dc": "[[extensionResourceId(resourceId('Microsoft.OperationalInsights/workspaces', parameters('workspaceName')), 'Microsoft.SecurityInsights/contentProductPackages', concat(variables('contentProductName'), '-dc'))]]",
    "_dataConnectorContentProductId_rdc": "[[extensionResourceId(resourceId('Microsoft.OperationalInsights/workspaces', parameters('workspaceName')), 'Microsoft.SecurityInsights/contentProductPackages', concat(variables('contentProductName'), '-rdc'))]]"
  }

}
