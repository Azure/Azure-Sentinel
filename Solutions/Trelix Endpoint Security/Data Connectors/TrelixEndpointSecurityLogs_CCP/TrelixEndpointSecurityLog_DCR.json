[
    {
        "name": "TrelixEndpointSecurityDCR",
        "apiVersion": "2021-09-01-preview",
        "type": "Microsoft.Insights/dataCollectionRules",
        "location": "{{location}}",
        "properties": {
            "dataCollectionEndpointId": "{{dataCollectionEndpointId}}",
            "streamDeclarations": {
                "Custom-TrelixESEvents": {
                    "columns": [
                        {
                            "name": "id",
                            "type": "string"
                        },
                       
                        {
                            "name": "attributes",
                            "type": "dynamic"
                        },
                        {
                            "name": "links",
                            "type": "dynamic"
                        },
                        {
                            "name": "relationships",
                            "type": "dynamic"
                        }
                    ]
                },
                "Custom-TrelixESAudit": {
                    "columns": [
                        {
                            "name": "id",
                            "type": "string"
                        },
                        
                        {
                            "name": "attributes",
                            "type": "dynamic"
                        },
                        {
                            "name": "links",
                            "type": "dynamic"
                        },
                        {
                            "name": "relationships",
                            "type": "dynamic"
                        }
                    ]
                },
                "Custom-TrelixESThreats": {
                    "columns": [
                        {
                            "name": "id",
                            "type": "string"
                        },
                        
                        {
                            "name": "attributes",
                            "type": "dynamic"
                        },
                        {
                            "name": "links",
                            "type": "dynamic"
                        },
                        {
                            "name": "relationships",
                            "type": "dynamic"
                        }
                    ]
                }
            },
            "destinations": {
                "logAnalytics": [
                    {
                        "workspaceResourceId": "{{workspaceResourceId}}",
                        "name": "clv2ws1"
                    }
                ]
            },
            "dataFlows": [
                {
                    "streams": [
                        "Custom-TrelixESEvents"
                    ],
                    "destinations": [
                        "clv2ws1"
                    ],
                    "outputStream": "TrelixEndpointSecurityEvents_CL"
                },
                {
                    "streams": [
                        "Custom-TrelixESAudit"
                    ],
                    "destinations": [
                        "clv2ws1"
                    ],
                    "outputStream": "TrelixEndpointSecurityAudit_CL"
                },
                {
                    "streams": [
                        "Custom-TrelixESThreats"
                    ],
                    "destinations": [
                        "clv2ws1"
                    ],
                    "outputStream": "TrelixEndpointSecurityThreats_CL"
                }
            ]
        }
    }
]