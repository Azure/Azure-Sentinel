                      },
                      {
                        "name": "links",
                        "type": "dynamic"
                      },
                      {
                        "name": "relationships",
                        "type": "dynamic"
                      }
                    ]
                  },
                  "Custom-TrelixESThreats": {
                    "columns": [
                      {
                        "name": "id",
                        "type": "string"
                      },
                      {
                        "name": "attributes",
                        "type": "dynamic"
                      },
                      {
                        "name": "links",
                        "type": "dynamic"
                      },
                      {
                        "name": "relationships",
                        "type": "dynamic"
                      }
                    ]
                  }
                },
                "destinations": {
                  "logAnalytics": [
                    {
                      "workspaceResourceId": "[variables('workspaceResourceId')]",
                      "name": "clv2ws1"
                    }
                  ]
                },
                "dataFlows": [
                  {
                    "streams": [
                      "Custom-TrelixESEvents"
                    ],
                    "destinations": [
                      "clv2ws1"
                    ],
                    "transformkql": "source |project TimeGenerated",
                    "outputStream": "Custom-TrelixEndpointSecurityEvents_CL"
                  },
                  {
                    "streams": [
                      "Custom-TrelixESAudit"
                    ],
                    "destinations": [
                      "clv2ws1"
                    ],
                    "transformkql": "source |project TimeGenerated",
                    "outputStream": "Custom-TrelixEndpointSecurityAudit_CL"
                  },
                  {
                    "streams": [
                      "Custom-TrelixESThreats"
                    ],
                    "destinations": [
                      "clv2ws1"
                    ],
                    "transformkql": "source |project TimeGenerated",
                    "outputStream": "Custom-TrelixEndpointSecurityThreats_CL"
                  }
                ]
              }
            },
            {
              "name": "TrelixEndpointSecurityAudit_CL",
              "apiVersion": "2022-10-01",
              "type": "Microsoft.OperationalInsights/workspaces/tables",
              "location": "[parameters('workspace-location')]",
