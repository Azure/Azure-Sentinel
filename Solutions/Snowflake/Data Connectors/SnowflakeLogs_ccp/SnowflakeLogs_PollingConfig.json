    "kind": "RestApiPoller",
    "properties": {
      "auth": {
        "type": "APIKey",
        "ApiKey": "{{apikey}}",
        "ApiKeyName": "Authorization",
        "ApiKeyIdentifier": "Bearer"
      },
      "request": {
        "apiEndpoint": "[[concat('https://',parameters('accountId'),'.snowflakecomputing.com','/api/v2/statements?retry=true')]",
        "httpMethod": "POST",
        "queryWindowInMin": 10,
