{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "mytimer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 */5 * * * *"
    },
    {
      "name": "checkpoint",
      "type": "blob",
      "dataType": "string",
      "path": "tir-checkpoints/checkpoint.txt",
      "direction": "in"
    },
    {
      "name": "$return",
      "type": "blob",
      "path": "tir-checkpoints/checkpoint.txt",
      "direction": "out"
    }
  ]
}
