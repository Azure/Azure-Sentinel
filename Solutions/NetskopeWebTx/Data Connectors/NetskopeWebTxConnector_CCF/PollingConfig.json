
{
    "name": "NetskopeWebTransactions",
    "apiVersion": "2022-12-01-preview",
    "type": "Microsoft.SecurityInsights/dataConnectors",
    "location": "{{location}}",
    "kind": "StorageAccountBlobContainer",
    "properties": {
        "connectorDefinitionName": "NetskopeWebTxConnector",
        "dcrConfig": {
            "streamName": "Custom-NetskopeWebTx"
        },
        "dataType": "NetskopeWebTransactions_CL",
        "auth": {
            "type": "ServicePrincipal"
        },
        "request": {
            "QueueUri": "{{QueueUri}}",
            "DlqUri": "{{DlqUri}}"
        },
        "response": {
            "eventsJsonPaths": [
                "$"
            ],
            "format": "csv",
            "isGzipCompressed": true
        }
    }
}
