[
    {
        "type": "Microsoft.SecurityInsights/dataConnectors",
        "apiVersion": "2022-10-01-preview",
        "name": "PaloAltoPrismaCloudAlert",
        "kind": "RestApiPoller",
        "properties": {
            "connectorDefinitionName": "PaloAltoPrismaCloudCSPMCCPDefinition",
            "dataType": "PaloAltoPrismaCloudAlertV2_CL",
            "dcrConfig": {
                "streamName": "Custom-PaloAltoPrismaCloudAlert",
                "dataCollectionEndpoint": "{{dataCollectionEndpoint}}",
                "dataCollectionRuleImmutableId": "{{dataCollectionRuleImmutableId}}"
            },
            "auth": {
                "type": "Bearer Token",
                "Token": "{{token}}",
                "prefix": "Bearer"
            },
            "request": {
                "apiEndpoint": "{{Base Url}}/alert/N-11?detailed=false&withAlertRuleInfo=false",
                "rateLimitQPS": 10,
                "queryWindowInMin": 5,
                "httpMethod": "Get",
                "queryTimeFormat": "yyyy-MM-ddTHH:mm:ssZ",
                "startTimeAttributeName": "from",
                "endTimeAttributeName": "to",
                "retryCount": 3,
                "timeoutInSeconds": 60,
                "headers": {
                    "Accept": "application/json",
                    "User-Agent": "Scuba"
                },
                "queryParameters": {
                    "sort": "time"
                }
            },
            "paging": {
                "pagingType": "Offset",
                "offsetParaName": "offset",
                "pageSizeParaName": "limit"
            }
        }
    },
    {
        "type": "Microsoft.SecurityInsights/dataConnectors",
        "apiVersion": "2022-10-01-preview",
        "name": "PaloAltoPrismaCloudAudit",
        "kind": "RestApiPoller",
        "properties": {
            "connectorDefinitionName": "PaloAltoPrismaCloudCSPMCCPDefinition",
            "dataType": "PaloAltoPrismaCloudAuditV2_CL",
            "dcrConfig": {
            "name": "PaloAltoPrismaCloudAuditV2_CL",
                "streamName": "Custom-PaloAltoPrismaCloudAudit",
                "dataCollectionEndpoint": "{{dataCollectionEndpoint}}",
                "dataCollectionRuleImmutableId": "{{dataCollectionRuleImmutableId}}"
            },
            "auth": {
                "type": "Bearer Token",
                "Token": "{{token}}",
                "prefix": "Bearer"
            },
            "request": {
                "apiEndpoint": "{{Base Url}}/audit/redlock",
                "rateLimitQPS": 10,
                "queryWindowInMin": 5,
                "httpMethod": "Get",
                "queryTimeFormat": "yyyy-MM-ddTHH:mm:ssZ",
                "startTimeAttributeName": "from",
                "endTimeAttributeName": "to",
                "retryCount": 3,
                "timeoutInSeconds": 60,
                "headers": {
                    "Accept": "application/json",
                    "User-Agent": "Scuba"
                },
                "queryParameters": {
                    "sort": "time"
