[
    {
        "name": "QscoutAppEventsConnector",
        "apiVersion": "2025-09-01",
        "type": "Microsoft.SecurityInsights/dataConnectors",
        "location": "{{location}}",
        "kind": "RestApiPoller",
        "properties": {
            "connectorDefinitionName": "QscoutAppEventsCCFDefinition",
            "dcrConfig": {
                "dataCollectionEndpoint": "{{dataCollectionEndpoint}}",
                "dataCollectionRuleImmutableId": "{{dataCollectionRuleImmutableId}}",
                "streamName": "Custom-QscoutAppEvents_CL"
            },
            "dataType": "QscoutAppEvents_CL",
            "response": {
                "eventsJsonPaths": [
                    "$"
                ],
                "format": "json"
            },
            "auth": {
                "type": "APIKey",
                "ApiKeyName": "x-api-key",
                "APIKey": "{{apiKey}}"
            },
            "request": {
                "apiEndpoint": "https://api.krwr.net/app-event-feed/export/{{organizationId}}",
                "rateLimitQPS": 10,
                "queryWindowInMin": 5,
                "queryTimeFormat": "yyyy-MM-dd'T'HH:mm:ss'.00Z'",
                "httpMethod": "GET",
                "retryCount": 3,
                "timeoutInSeconds": 60,
                "headers": {
                    "Accept": "application/json",
                    "User-Agent": "Microsoft-Azure-Sentinel-QscoutAppEventsCCFDefinition"
                }
            },
            "isActive": true
        }
    }
]