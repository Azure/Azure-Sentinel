{
    "name": "QscoutAppEventsConnector",
    "apiVersion": "2022-12-01-preview",
    "type": "Microsoft.SecurityInsights/dataConnectors",
    "kind": "RestApiPoller",
    "properties": {
        "dataType": "QscoutAppEvents_CL",
        "response": {
            "eventsJsonPaths": ["$"],
            "format": "json"
        },
        "connectorDefinitionName": "QscoutAppEventsCCFDefinition",
        "auth": {
            "ApiKey": "{{apiKey}}",
            "ApiKeyName": "x-api-key",
            "ApiKeyIdentifier": "ApiToken",
            "type": "APIKey"
        },
        "request": {
            "apiEndpoint": "http://api.krwr.net/app-event-feed/export/{{organizationId}}",
            "RateLimitQPS": 10,
            "QueryWindowInMin": 5,
            "HttpMethod": "GET",
            "RetryCount": 3,
            "TimeoutInSeconds": 60,
            "Headers": {
            "Accept": "*/*"
            }
        },
        "dcrConfig": {
            "streamName": "Custom-AppEvents_CL",
            "dataCollectionEndpoint": "{{dataCollectionEndpoint}}",
            "dataCollectionRuleImmutableId": "{{dataCollectionRuleImmutableId}}"
        },
        "isActive": true
    }
}