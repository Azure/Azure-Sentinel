          {
            "type": 3,
            "content": {
              "version": "KqlItem/1.0",
              "query": "Talon_CL\n| extend SplitAll=split(url_s, '/')[0]\n| summarize AggregatedValue = count() by tostring(SplitAll)\n| where tostring(SplitAll) != ''\n| order by AggregatedValue",
