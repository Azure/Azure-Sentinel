tostring(sublocation_id), tostring(['sublocation-id'])),\r    PktSrcAwsService = iff(isnotempty(tostring(pkt_src_aws_service)), tostring(pkt_src_aws_service), tostring(['pkt-src-aws-service'])),\r    PktDstAwsService = iff(isnotempty(tostring(pkt_dst_aws_service)), tostring(pkt_dst_aws_service), tostring(['pkt-dst-aws-service'])),\r    FlowDirection = iff(isnotempty(tostring(flow_direction)), tostring(flow_direction), tostring(['flow-direction'])),\r    TrafficPath = iff(isnotempty(tostring(traffic_path)), tostring(traffic_path), tostring(['traffic-path'])),\r    EcsClusterArn = iff(isnotempty(tostring(ecs_cluster_arn)), tostring(ecs_cluster_arn), tostring(['ecs-cluster-arn'])),\r    EcsClusterName = iff(isnotempty(tostring(ecs_cluster_name)), tostring(ecs_cluster_name), tostring(['ecs-cluster-name'])),\r    EcsContainerInstanceArn = iff(isnotempty(tostring(ecs_container_instance_arn)), tostring(ecs_container_instance_arn), tostring(['ecs-container-instance-arn'])),\r    EcsContainerInstanceId = iff(isnotempty(tostring(ecs_container_instance_id)), tostring(ecs_container_instance_id), tostring(['ecs-container-instance-id'])),\r    EcsContainerId = iff(isnotempty(tostring(ecs_container_id)), tostring(ecs_container_id), tostring(['ecs-container-id'])),\r    EcsSecondContainerId = iff(isnotempty(tostring(ecs_second_container_id)), tostring(ecs_second_container_id), tostring(['ecs-second-container-id'])),\r    EcsServiceName = iff(isnotempty(tostring(ecs_service_name)), tostring(ecs_service_name), tostring(['ecs-service-name'])),\r    EcsTaskDefinitionArn = iff(isnotempty(tostring(ecs_task_definition_arn)), tostring(ecs_task_definition_arn), tostring(['ecs-task-definition-arn'])),\r    EcsTaskArn = iff(isnotempty(tostring(ecs_task_arn)), tostring(ecs_task_arn), tostring(['ecs-task-arn'])),\r    EcsTaskId = iff(isnotempty(tostring(ecs_task_id)), tostring(ecs_task_id), tostring(['ecs-task-id']))",
