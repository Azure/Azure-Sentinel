  "validation_framework": {
    "required_fields": {
      "all_events": ["id", "enterprise_guid", "created_time", "type"],
      "threat_events": ["threat.id", "threat.classification", "threat.risk", "threat.status"],
      "device_events": ["device.guid", "device.platform", "device.security_status"],
      "smishing_alert_events": ["smishing_alert.id", "smishing_alert.type", "smishing_alert.severity"],
      "audit_events": ["audit.type", "audit.attribute_changes"]
    },
    "field_extraction_tests": {
      "datetime_parsing": ["created_time", "activated_at", "checkin_time"],
      "string_normalization": ["platform", "os_version", "manufacturer", "model"],
      "dynamic_object_preservation": ["device", "threat", "audit", "smishing_alert"],
      "array_handling": ["device_permissions", "attribute_changes", "detections"]
    }
  }
}