[
    {
        "name": "LookoutOldEndpointPoller",
        "type": "Microsoft.SecurityInsights/dataConnectors",
        "apiVersion": "2022-12-01-preview",
        "location": "{{location}}",
        "kind": "RestApiPoller",
        "properties": {
            "connectorDefinitionName": "LookoutOldEndpoint_ConnectorDefinition",
            "dataType": "LookoutOldEventsV2_CL",
            "dcrConfig": {
                "streamName": "Custom-LookoutOldEventsV2_CL",
                "dataCollectionEndpoint": "{{dataCollectionEndpoint}}",
                "dataCollectionRuleImmutableId": "{{dataCollectionRuleImmutableId}}"
            },
            "auth": {
                "type": "OAuth2",
                "ClientId": "NA",
                "ClientSecret": "NA",
                "grantType": "client_credentials",
                "tokenEndpoint": "https://api.lookout.com/oauth2/token",
                "tokenEndpointHeaders": {
                    "Content-Type": "application/x-www-form-urlencoded",
                    "Accept": "application/json",
                    "Authorization": "[[concat('Bearer ',parameters('applicationKey'))]"
                },
                "TokenEndpointQueryParameters": {}
            },
            "request": {
                "apiEndpoint": "https://api.lookout.com/events",
                "httpMethod": "GET",
                "rateLimitQps": 5,
                "retryCount": 3,
                "timeoutInSeconds": 60,
                "queryWindowInMin": 5,
                "queryTimeFormat": "yyyy-MM-ddTHH:mm:ssZ",
                "headers": {
                    "Accept": "application/json",
                    "User-Agent": "Scuba"
                },
                "queryParameters": {
                    "eventType": "DEVICE,THREAT,AUDIT"
                }
            },
            "response": {
                "eventsJsonPaths": [
                    "$.events"
                ],
                "format": "json"
            },
            "paging": {
                "pagingType": "PersistentToken",
                "nextPageTokenJsonPath": "$.streamPosition",
                "nextPageParaName": "streamPosition",
                "pageSize": 100,
                "pageSizeParameterName":"limit"
            }
        }
    }
]