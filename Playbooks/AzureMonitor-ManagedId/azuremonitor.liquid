{% assign rows = content.tables[0].rows %}
{% assign columns = content.tables[0].columns %}
{% assign index = columns.size | minus: 1 %}
[
    {% for row in rows %}
        {
            {% for num in (0..index) %}
            {% if num == index %}
            {% break %}
            {% endif %}
                "{{columns[num].name}}": "{{row[num] | Replace: '"', '\"'}}",
            {% endfor %}
        },
    {% endfor %}
]
