                                      },
                                      "type": "array"
                                    },
                                    "settings_hash": {
                                      "type": "string"
                                    }
                                  },
                                  "required": [
                                    "policy_type",
                                    "policy_id",
                                    "applied",
                                    "settings_hash",
                                    "assigned_date",
                                    "applied_date",
                                    "rule_groups"
                                  ],
                                  "type": "object"
                                },
                                "type": "array"
                              },
                              "product_type": {
